{
  "name": "<project-name>",
  "protocol": "sftp",
  "port": 22300,
  "username": "<username>",
  "password": "<password>",
  "remotePath": "/srv/<username>/",
  "uploadOnSave": false,
  "useTempFile": false,
  "openSsh": true,
  "profiles": {
    "root": {
      "host": "<host>",
      "remotePath": "/srv/<username>/"
    },
    "<project-name>": {
      "host": "<host>",
      "remotePath": "/srv/<username>/<project-name>/"
    }
  },
  "defaultProfile": "<project-name>"
}
